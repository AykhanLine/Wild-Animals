@model SignUp

@if(Model == null)
{
   <a class="btn btn-outline-success btn-sm my-4" href="/dashboard/signup/create">Create</a>
}


@if (Model != null)
{
    <table class="table table-striped table-dark">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Title</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>

            <tr>
                <th scope="row">@Model.Id</th>
                <td>@Model.Title</td>
               
                <td>
                    <a href="/dashboard/signup/edit/@Model.Id" class="btn btn-outline-warning btn-sm">Edit</a>
                    <a href="/dashboard/signup/detail/@Model.Id" class="btn btn-outline-info btn-sm">Detail</a>
                    <a href="/dashboard/signup/delete/@Model.Id" class="btn btn-outline-danger btn-sm">Delete</a>
                </td>
            </tr>


        </tbody>
    </table>
}

